<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Notification\MessageList">
        <arguments>
            <argument name="messages" xsi:type="array">
                <item name="mageme_activate_message" xsi:type="string">MageMe\Core\Notification\License\Activate</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Config\Model\Config\Structure\Data">
        <plugin name="mageme_config" type="MageMe\Core\Plugin\Magento\Config\Model\Config\Structure\Data\ConfigMerge"/>
    </type>
</config>
