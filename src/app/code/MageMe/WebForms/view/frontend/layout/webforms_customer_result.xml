<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="MageMe\WebForms\Block\Customer\Account\Result\View"
                   name="webforms.customer.account.result.view"
                   template="customer/account/result/view.phtml" cacheable="false">
                <block class="MageMe\WebForms\Block\Customer\Account\Result\MessageForm"
                       name="webforms.customer.account.result.view.message_form"
                       template="customer/account/result/message_form.phtml" cacheable="false"/>
                <block class="MageMe\WebForms\Block\Customer\Account\Result\Messages"
                       name="webforms.customer.account.result.view.messages"
                       template="customer/account/result/messages.phtml" cacheable="false"/>
            </block>
        </referenceContainer>
    </body>
</page>
